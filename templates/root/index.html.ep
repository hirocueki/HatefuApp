% layout 'default';
% title 'ようこそハテフへ';
<h2><%= $msg %></h2>


<form method="post" action="/">
  <div class="form-group">
    <label for="tag">TAG</label>
    <input type="text" class="form-control" id="keyword" name="keyword" placeholder="Enter Tag" />
    <input type="text" value="200" class="form-control" id="threshold" name="threshold" placeholder="Enter Threshold" />
  </div>
  <button type="submit" class="btn btn-default">ハテフ!</button>
</form>
